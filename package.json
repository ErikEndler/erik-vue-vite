{
  "name": "erik-vue-vite",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "db:serve": "json-server --watch db.json",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:ui": "vitest --ui",
    "test:unit-commit": "vitest --run",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore",
    "format": "prettier --write ./",
    "prepare": "husky"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "json-server": "^0.17.4",
    "pinia": "^3.0.3",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1",
    "wait-for-expect": "^4.0.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "commitlint-plugin-function-rules": "^4.0.2",
    "cypress": "^14.5.4",
    "eslint": "^9.33.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "^10.4.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "start-server-and-test": "^2.0.13",
    "typescript": "~5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.4",
    "vue-tsc": "^3.0.5"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  }
}
